<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eoe.drugstore">

    <!-- 各个权限声明 -->
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_learn"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".BroadCastReceiver.MyReceiver">
            <intent-filter android:priority="20">
                <action android:name="com.eoe.drugstore.fragment.communityFragment" />
            </intent-filter>
        </receiver>
        <receiver android:name=".BroadCastReceiver.MyReceiver2">
            <intent-filter>
                <action android:name="com.eoe.drugstore.fragment.communityFragment" />
            </intent-filter>
        </receiver>


    </application>

</manifest>